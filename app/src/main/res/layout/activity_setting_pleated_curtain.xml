<ScrollView
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:scrollbars="none"
    android:layout_weight="1"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
        android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        android:paddingBottom="@dimen/activity_vertical_margin"
        tools:context="com.first.triparky.caldecorate.setting"
        android:background="#34495e"

        >


        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="fill_parent">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="การจับจีบผ้า(เท่า)"
                android:id="@+id/textView4"
                android:layout_column="0"
                android:textColor="#fff" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="fill_parent">

            <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="number"
                android:ems="10"
                android:id="@+id/setD_edittext"
                android:hint="2.5 ซม."
                android:textColor="#f39c12"
                android:textColorHint="#afa3a3a3" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="fill_parent">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="*พับเย็บผ้าเคมี(ซม.)"
                android:id="@+id/textView5"
                android:layout_column="0"
                android:textColor="#fff" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="fill_parent">

            <EditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:inputType="numberDecimal"
                android:ems="10"
                android:id="@+id/setE_edittext"
                android:layout_column="0"
                android:hint="10 ซม"
                android:textColorHint="#afa3a3a3"
                android:layout_weight="1"
                android:textColor="#f39c12" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="fill_parent">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="*พับเย็บผ้าใส้โซ่(ซม.)"
                android:id="@+id/textView6"
                android:layout_column="0"
                android:textColor="#fff" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="fill_parent">

            <EditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:inputType="numberDecimal"
                android:ems="10"
                android:id="@+id/setF_edittext"
                android:layout_column="0"
                android:hint="10 ซม"
                android:textColorHint="#afa3a3a3"
                android:layout_weight="1"
                android:textColor="#f39c12" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="fill_parent">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="*เผื่อความกว้าง(ซม.)"
                android:id="@+id/textView7"
                android:layout_column="0"
                android:textColor="#fff" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="fill_parent">

            <EditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:inputType="numberDecimal"
                android:ems="10"
                android:id="@+id/setG_edittext"
                android:layout_column="0"
                android:hint="30 ซม"
                android:textColorHint="#afa3a3a3"
                android:layout_weight="1"
                android:textColor="#f39c12" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="fill_parent">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="*ชายผ้าด้านล่าง(ซม.)"
                android:id="@+id/textView8"
                android:layout_column="0"
                android:textColor="#fff" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="fill_parent">

            <EditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:inputType="numberDecimal"
                android:ems="10"
                android:id="@+id/setH_edittext"
                android:layout_column="0"
                android:hint="30 ซม"
                android:textColorHint="#afa3a3a3"
                android:layout_weight="1"
                android:textColor="#f39c12" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="fill_parent">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="เผื่อจำนวนผ้า(%)"
                android:id="@+id/textView9"
                android:layout_column="0"
                android:textColor="#fff" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="fill_parent">

            <Spinner
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/setI_spin"
                android:layout_column="0"
                android:layout_weight="1" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="fill_parent">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="*สายรวบม่าน(ซม.)         2  เส้น"
                android:id="@+id/textView"
                android:layout_column="0"
                android:textColor="#fff" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="fill_parent">

            <EditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:inputType="numberDecimal"
                android:ems="10"
                android:id="@+id/setJ_edittext"
                android:layout_column="0"
                android:hint="110 ซม"
                android:textColorHint="#afa3a3a3"
                android:layout_weight="1"
                android:textColor="#f39c12" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="fill_parent">

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="fill_parent">

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="fill_parent">

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="fill_parent">

        </TableRow>

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">

        </TableRow>

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">

        </TableRow>

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">

        </TableRow>

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">

            <Button
                style="?android:attr/buttonStyleSmall"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:text="บันทึกการเปลี่ยนแปลง"
                android:id="@+id/changeSet_button"
                android:layout_column="0"
                android:enabled="true"
                android:textColor="#ffffffff"
                android:layout_weight="1"
                android:background="#27ae60"
                android:textSize="20dp" />
        </TableRow>

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">

            <Button
                style="?android:attr/buttonStyleSmall"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:text="ใช้ค่ามาตรฐาน"
                android:id="@+id/noChSet_button"
                android:layout_column="0"
                android:textColor="#ffffffff"
                android:layout_weight="1"
                android:background="#3498db"
                android:layout_marginTop="5dp"
                android:textSize="20dp" />
        </TableRow>

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">

        </TableRow>

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">

        </TableRow>

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">

        </TableRow>
    </TableLayout>
</ScrollView>
